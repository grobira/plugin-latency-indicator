    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1",
    "flex-dev-utils": "^4.5.0-beta.0",
    "jest": "^26.4.2",
    "jest-environment-jsdom-fourteen": "^1.0.1",
    "jest-watch-typeahead": "^0.6.1",
    "react-app-polyfill": "^1.0.6",
    "react-test-renderer": "^16"
  },
  "devDependencies": {
    "@jest/types": "^26.5.2"
  },
  "engines": {
    "node": ">=10.12.0"
